<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="kievreclama.task.entity.Task" table="tasks">
        <id name="id" column="id" type="integer">
            <generator class="increment"/>
        </id>
        <property name="date" column="date_create" type="date"/>
        <property name="delete" column="delete" type="boolean"/>
        <property name="emploue" column="emploue" type="integer"/>
        <property name="infoTask" column="task" type="string"/>
        <property name="number" column="number" type="string"/>
        <property name="priority" column="urgency" type="integer"/>
        <property name="statys" column="state" type="boolean"/>
    </class>
</hibernate-mapping>
